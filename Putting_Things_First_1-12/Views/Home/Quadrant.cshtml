@{
    ViewData["Title"] = "Quadrant Page";
}

<div class="container mt-4">
    <h1 class="text-center mb-4">Time Management Matrix</h1>
    <div class="row text-center">
        <!-- Quadrant I: Important / Urgent -->
        <div class="col-md-6 mb-4">
            <div class="p-3 border bg-light">
                <h2>Quadrant I</h2>
                <p>Important and Urgent</p>
                <!-- Display tasks for Quadrant I here -->

            </div>
        </div>
        <!-- Quadrant II: Important / Not Urgent -->
        <div class="col-md-6 mb-4">
            <div class="p-3 border bg-light">
                <h2>Quadrant II</h2>
                <p>Important but Not Urgent</p>
                <!-- Display tasks for Quadrant II here -->
            </div>
        </div>
        <!-- Quadrant III: Not Important / Urgent -->
        <div class="col-md-6 mb-4">
            <div class="p-3 border bg-light">
                <h2>Quadrant III</h2>
                <p>Not Important but Urgent</p>
                <!-- Display tasks for Quadrant III here -->
            </div>
        </div>
        <!-- Quadrant IV: Not Important / Not Urgent -->
        <div class="col-md-6 mb-4">
            <div class="p-3 border bg-light">
                <h2>Quadrant IV</h2>
                <p>Not Important and Not Urgent</p>
                <!-- Display tasks for Quadrant IV here -->
            </div>
        </div>
    </div>
</div>
